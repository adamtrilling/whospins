%a.brand{:href => '/'}
  = image_tag 'logo.png', :width => 60
= link_to "Who Spins?", root_path, :class => 'brand'
%ul.nav.pull-right
  - if user_signed_in?
    %li
      %a{'href' => '#loginModal', 'data-toggle' => 'modal'} Add Accounts
    %li
      = link_to 'Log Out', logout_path
  - else
    %li
      %a{'href' => '#loginModal', 'data-toggle' => 'modal'} Log In

=render partial: 'layouts/login'